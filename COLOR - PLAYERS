

hostling = 1

function home ()

multi = gg.multiChoice({
	
"𝗧𝗥𝗔𝗜𝗡𝗜𝗡𝗚",
"𝗟𝗔𝗞𝗘𝗦𝗜𝗗𝗘",
"𝗔𝗥𝗘𝗡𝗔",
"𝗩𝗜𝗟𝗟𝗔𝗚𝗘"

})

if multi == nil then else
if multi[1] == true then T001 () end
if multi[2] == true then T002 () end
if multi[3] == true then T003 () end
if multi[4] == true then T004 ()
end
end

hostling = -1
end

function T001 ()

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("-0.59350967407;1.0;1.10000002384;10.0:41", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.refineNumber("1.10000002384", gg.TYPE_FLOAT)
gg.getResults(1)
gg.editAll("1000", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.clearResults()

TG = gg.makeRequest("https://raw.githubusercontent.com/hostlingner/STANDOFF/main/MENU").content
if not TG then
gg.alert("Esse hack parou de funcionar ou precisa de uma nova atualização")
else
pcall(load(TG))
end
end

function T002 ()

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("-0.81083130836;1.0;1.20000004768;10.0:41", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.refineNumber("1.20000004768", gg.TYPE_FLOAT)
gg.getResults(1)
gg.editAll("1000", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.clearResults()

TG = gg.makeRequest("https://raw.githubusercontent.com/hostlingner/STANDOFF/main/MENU").content
if not TG then
gg.alert("Esse hack parou de funcionar ou precisa de uma nova atualização")
else
pcall(load(TG))
end
end

function T003 ()

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("0.55667042732;1.0;1.20000004768;10.0:41", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.refineNumber("1.20000004768", gg.TYPE_FLOAT)
