M2 = off
M3 = off
M4 = off
M5 = off
M6 = off

if gg.isVisible(true) then
gg.setVisible(false)
end

function main()

menu = gg.choice({ 

M1 .. "        ğ—™ğ—Ÿğ—¨ğ—§ğ—¨ğ—”ğ—¥ ğ—˜ ğ—£ğ—¨ğ—Ÿğ—”ğ—¥\n",
M2 .. "        ğ—›ğ—œğ—§ ğ—•ğ—¢ğ—«\n",
M3 .. "        ğ—£ğ—˜ğ—¥ğ—¦ğ—¢ğ—¡ğ—”ğ—šğ—˜ğ—  ğ—£ğ—¥ğ—˜ğ—§ğ—¢\n",
M4 .. "        ğ—–ğ—˜ğ—¨ ğ—£ğ—¥ğ—˜ğ—§ğ—¢\n",
M5 .. "        ğ—¦ğ—˜ğ—  ğ—¥ğ—˜ğ—–ğ—¢ğ—œğ—Ÿ ğ—”ğ—¥ğ— ğ—”ğ—¦\n",
M6 .. "        ğ—–ğ—¢ğ—¥ ğ—£ğ—Ÿğ—”ğ—¬ğ—˜ğ—¥ğ—¦\n"

}, nil, "")

if menu == 1
then
if M1 == off
then
M1 = on
MFunc1(true)
else
M1 = off
MFunc1(false)
end
end

if menu == 2
then
if M2 == off
then
M2 = on
MFunc2(true)
else
M2 = off
MFunc2(false)
end
end

if menu == 3
then
if M3 == off
then
M3 = on
MFunc3(true)
else
M3 = off
MFunc3(false)
end
end

if menu == 4
then
