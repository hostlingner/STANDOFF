function MFunc4(t)

if t == true then

gg.setRanges(gg.REGION_VIDEO)
gg.searchNumber("1080559619", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
revert = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("1380559619", gg.TYPE_DWORD)
gg.clearResults()

gg.toast("Ativado")

else

gg.setRanges(gg.REGION_VIDEO)
gg.searchNumber("1380559619", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
revert = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("1080559619", gg.TYPE_DWORD)
gg.clearResults()

gg.toast("Desativado")

end
end

function MFunc5(t)

if t == true then

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("1.0;2.20000004768:5", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.refineNumber("1.0", gg.TYPE_FLOAT)
gg.getResults(100)
gg.editAll("0", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.clearResults()

gg.toast("Ativado")

else

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("0;2.20000004768:5", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.refineNumber("0", gg.TYPE_FLOAT)
gg.getResults(100)
gg.editAll("1.0", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.clearResults()

gg.toast("Desativado")

end
end

function MFunc6(t)

if t == true then

TG = gg.makeRequest("https://raw.githubusercontent.com/hostlingner/STANDOFF/main/COLOR%20-%20PLAYERS").content
if not TG then
gg.alert("Esse hack parou de funcionar ou precisa de uma nova atualizaÃ§Ã£o")
else
pcall(load(TG))
end

else

gg.toast("Nada por aqui")

end
end

while true do
if gg.isVisible(true)
then
gg.setVisible(false)
mbym = 1
end
if mbym == 1 then
main()
end
end
